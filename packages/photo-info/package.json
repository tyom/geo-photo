{
  "name": "photo-info",
  "version": "0.0.1-rc.1",
  "type": "module",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "prerelease": "pnpm version prerelease --preid=rc && git add package.json && git commit -m \"$(cat package.json | jq -r '.version')\" && git tag v$(cat package.json | jq -r '.version')"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "exifreader": "^4.23.7"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "vitest": "^2.1.3"
  }
}
